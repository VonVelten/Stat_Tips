@article{saffar_soflaei_hookah_2018,
	title = {Hookah smoking is strongly associated with diabetes mellitus, metabolic syndrome and obesity: a population-based study},
	volume = {10},
	issn = {1758-5996},
	shorttitle = {Hookah smoking is strongly associated with diabetes mellitus, metabolic syndrome and obesity},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5907743/},
	doi = {10.1186/s13098-018-0335-4},
	abstract = {Objectives
The adverse effects of cigarette smoking have been widely studied before, whilst the effects of hookah smoking has received less attention, although it is a common habit in the Middle East. Here we have investigated the effects of cigarette and hookah smoking on biochemical characteristics in a representative population sample derived from the Mashhad stroke and heart atherosclerotic disorder (MASHAD) cohort study, from Northeastern Iran.

Study design
A total of 9840 subjects from the MASHAD population study were allocated to five groups; non-smokers (6742), ex-smokers (976), cigarette smokers (864), hookah smokers (1067), concomitant cigarette and hookah smokers (41).

Methods
Baseline characteristics were recorded in a questionnaire. Biochemical characteristics were measured by routine methods. Data were analyzed using SPSS software and p {\textless} 0.05 was considered significant.

Results
After adjustment for age and sex; the presence of CVD, obesity, metabolic syndrome, DM and dyslipidemia were significantly (p {\textless} 0.001) related to smoking status. After multivariate analysis, HDL (p {\textless} 0.001), WBC (p {\textless} 0.001), MCV (p {\textless} 0.05), PLT (p {\textless} 0.01) and RDW (p {\textless} 0.001), and the presence of CVD (p {\textless} 0.01), obesity (p {\textless} 0.001), metabolic syndrome (p {\textless} 0.05) and DM (p {\textless} 0.01) remained significant between cigarette smokers and non-smokers. Between hookah smokers and non-smokers; uric acid (p {\textless} 0.001), PLT (p {\textless} 0.05) and RDW (p {\textless} 0.05), and the presence of obesity (p {\textless} 0.01), metabolic syndrome (p {\textless} 0.001), diabetes (p {\textless} 0.01) and dyslipidemia (p {\textless} 0.01) remained significant after logistic regression.

Conclusion
There was a positive association between hookah smoking and metabolic syndrome, diabetes, obesity and dyslipidemia which was not established in cigarette smoking.},
	urldate = {2019-07-26},
	journal = {Diabetology \& Metabolic Syndrome},
	author = {Saffar Soflaei, Sara and Darroudi, Susan and Tayefi, Maryam and Nosrati Tirkani, Abolfazl and Moohebati, Mohsen and Ebrahimi, Mahmoud and Esmaily, Habibollah and Parizadeh, Seyed Mohammad Reza and Heidari-Bakavoli, Ali Reza and Ferns, Gordon A. and Ghayour-Mobarhan, Majid},
	year = {2018},
	pmid = {29713386},
	pmcid = {PMC5907743},
	file = {PubMed Central Full Text PDF:C\:\\Users\\p.gaignon\\Zotero\\storage\\8HX7TMZL\\Saffar Soflaei et al. - 2018 - Hookah smoking is strongly associated with diabete.pdf:application/pdf}
}

