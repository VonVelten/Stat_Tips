	keywords = {Phosphorus, hypocalcemia, Pregnancy, Calcium, Female, Animal Feed, Animals, Cattle, Medicago sativa, Anions, Parturient Paresis, Sodium, Cattle Diseases, Labor, Obstetric, Cations},
	pages = {2525--2531},
	file = {Gaynor et al. - 1989 - Parturient hypocalcemia in jersey cows fed alfalfa.pdf:C\:\\Users\\p.gaignon\\Zotero\\storage\\UM42F4J2\\Gaynor et al. - 1989 - Parturient hypocalcemia in jersey cows fed alfalfa.pdf:application/pdf}
}

