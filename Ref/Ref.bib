@incollection{blanco_chapter_2017,
	title = {Chapter 29 - {Essential} {Minerals}},
	isbn = {978-0-12-803550-4},
	url = {http://www.sciencedirect.com/science/article/pii/B978012803550400029X},
	abstract = {Sodium is the main cation of the extracellular fluid (ECF) (55\% of the total Na+ is in the ECF); the rest is located mainly in bone (40\%). Plasma Na+ concentration is 135–145 mEq./L; intracellular Na+ only reaches 10 mEq./L. Na+ contributes to half of the osmolarity of ECF and drives fluid movement between different body compartments. The major route for Na+ excretion is the kidney. Aldosterone is the main regulator of Na+ excretion in the kidney. Atrial natriuretic peptides also contribute to Na+ homeostasis. Potassium is the main cation of the intracellular space (90\% of the total K+ is in the ICF), with a concentration of ∼150 mEq./L. In plasma, K+ concentration is 4–5.5 mEq./L. K+ is eliminated in the urine in an aldosterone-regulated manner. Chloride is the main anion of the ECF, corresponding to 88\% of the total body Cl−. Plasma Cl− concentration is 102 mEq./L. Cl− is Na+ main attending anion; its amounts follow those of Na+. Cl− contributes importantly to plasma osmolarity. Its regulation is dependent on that of Na+. Calcium is abundant in bone. Plasma Ca2+ concentration is ∼5 mEq./L, 50\% of which exists as free Ca2+ and the rest is bound to proteins or forms nonionizable compounds. It is regulated by parathyroid hormone and calcitriol. Phosphorus is mainly located in bone (over 80\% of the total P) forming part of bone hydroxyapatite. In the ECF, phosphorus exists partly as phosphate anions ( HP O 4 2 − / H 2 P O 4 − ) ; the plasma concentration is 2.5–4.8 mEq./L. Calcitriol and parathyroid hormone are regulators of body phosphorus levels. Magnesium is an essential cation, involved in many enzymatic reactions. Its plasma concentration is 1.25–2.5 mEq./L. Iron is mainly bound to proteins in the body, including hemoglobin, ferritin, hemosiderin, and transferrin. Fe is continuously recycled in the body, particularly during the lysis and production of erythrocytes. Fe is eliminated mainly by the intestine. An overload of Fe results in formation of hemosiderin granules and hemosiderosis. Lack of Fe in the diet produces hypochromic microcytic anemia. Zinc is a constituent of many enzymes. Copper forms part of enzymes, such as cytochrome oxidase, superoxide dismutase, monoamine oxidase, tyrosinase, and ceruloplasmin. Iodine is almost exclusively required for the synthesis of thyroid hormones. Manganese is required as a cofactor of some enzymes. Molybdenum is an important component of several metalloenzymes. Selenium is associated with two amino acids, selenomethionine and selenocysteine, which form part of different proteins, such as glutathione peroxidase, iodotyrosine deiodinases, thioredoxin reductase, and synthetase selenophosphate.},
	urldate = {2017-05-04},
	booktitle = {Medical {Biochemistry}},
	publisher = {Academic Press},
	author = {Blanco, Antonio and Blanco, Gustavo},
	year = {2017},
	doi = {10.1016/B978-0-12-803550-4.00029-X},
	keywords = {Phosphorus, Calcium, ceruloplasmin, chloride, copper, ferritin, hemosiderin, hemosiderosis, hydroxyapatite, hypochromic microcytic anemia, iodine, iron, manganese, molybdenum, Potassium, Selenium, Sodium, transferrin, Zinc},
	pages = {715--743}
}

@incollection{blanco_chapter_2017-1,
