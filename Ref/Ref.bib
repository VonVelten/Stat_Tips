@incollection{nurit_mammary_2016,
	title = {Mammary {Gland}, {Milk} {Biosynthesis} and {Secretion}: {Secretion} of {Milk} {Constituents}},
	isbn = {978-0-08-100596-5},
	shorttitle = {Mammary {Gland}, {Milk} {Biosynthesis} and {Secretion}},
	url = {http://www.sciencedirect.com/science/article/pii/B978008100596500888X},
	abstract = {The mechanisms underlying the secretion of the major components of milk from mammary epithelial cells are discussed. The hallmarks of milk-secreting cells include the assembly and secretion of lipid droplets coated with cellular membranes, synthesis of lactose, which indirectly regulates the secretion of water, and the assembly of caseins with calcium and phosphate into micellar aggregates. Milk constituents are either synthesized in the mammary gland or transported across the epithelial barrier from other sources. At least five major pathways have been identified: (1) secretion of membrane-coated lipid droplets from the apical surface; (2) packaging of most skim milk proteins, lactose, ions, and water in secretory vesicles, followed by secretion from the apical plasma membrane by exocytosis; (3) transport of proteins, including immunoglobulins and peptide hormones, from other sources by transcytosis; (4) equilibration of ions and water across the interstitial space, cell cytoplasm, and milk by specific transporters; and (5) paracellular transit of material between cells when the epithelial tight junctions are permeable, for example, during late pregnancy, late lactation, and at times of bacterial inflammation. In addition, membrane-bounded vesicles containing cytoplasmic proteins, called ‘exosomes,’ may be released from the apical surface by poorly characterized mechanisms.},
	urldate = {2016-03-10},
	booktitle = {Reference {Module} in {Food} {Science}},
	publisher = {Elsevier},
	author = {Nurit, A. and Mather, I. H.},
	year = {2016},
	keywords = {Casein micelle, Exocytosis, Golgi complex, Ion transport, Lactose synthesis, Milk fat globule membrane, Milk lipid secretion, Secretory pathway, Water secretion}
}

