	keywords = {Phosphorus, Parathyroid Hormone, Calcium, Female, Humans, Animals, Calcium, Dietary, Cattle, Feces, Kinetics, Homeostasis, Age Factors, Radioimmunoassay, Biological Transport, Feedback},
