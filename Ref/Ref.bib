	publisher = {Russell Sage Foundation},
	editor = {Cooper, Harris and Hedges, Larry V. and Valentine, Jeffrey C.},
	year = {2009},
	file = {Cooper et al. - 2009 - The Handbook of Research Synthesis and Meta-Analys.pdf:C\:\\Users\\p.gaignon\\Zotero\\storage\\3WD5L8PQ\\Cooper et al. - 2009 - The Handbook of Research Synthesis and Meta-Analys.pdf:application/pdf}
