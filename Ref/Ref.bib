	urldate = {2017-05-19},
	journal = {Journal of Food Composition and Analysis},
	author = {Rodrıguez Rodrıguez, E. M. and Sanz Alaejos, M. and Dıaz Romero, C.},
	year = {2001},
	keywords = {Calcium, Magnesium, copper, iron, Potassium, Selenium, Sodium, Zinc, cow's milk.},
