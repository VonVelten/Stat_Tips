	author = {Lopez, H. and Kanitz, F. D. and Moreira, V. R. and Wiltbank, M. C. and Satter, L. D.},
	year = {2004},
	pmid = {14765820},
	keywords = {Phosphorus, Lactation, Pregnancy, Female, Animals, Cattle, Medicago sativa, Silage, Zea mays, Nutritional Requirements, Diet, Milk, Digestion, Phosphorus, Dietary, Body Composition, Lipids, Milk Proteins},
	pages = {139--145},
	file = {Lopez et al. - 2004 - Effect of dietary phosphorus on performance of lac.pdf:C\:\\Users\\p.gaignon\\Zotero\\storage\\8TWB5IM5\\Lopez et al. - 2004 - Effect of dietary phosphorus on performance of lac.pdf:application/pdf}
