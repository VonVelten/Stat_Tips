	pages = {122--124},
	file = {Reinhardt et al. - 2011 - Prevalence of subclinical hypocalcemia in dairy he.pdf:C\:\\Users\\p.gaignon\\Zotero\\storage\\7BBPENUZ\\Reinhardt et al. - 2011 - Prevalence of subclinical hypocalcemia in dairy he.pdf:application/pdf}
}

@article{romo_blood_1991,
