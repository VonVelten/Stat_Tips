	doi = {10.1111/j.1439-0396.1993.tb00785.x},
	abstract = {Zusammenfassung Wirkungen niedriger P?Versorgung bei normalen und hohen Ca?Gehalten im Futter von Milchziegen auf Leistung und Mineralstoff?Bilanzen während Trächtigkeit und Laktation An Milchziegen wurde ein 2?faktorieller Versuch (2 times 2) von der 7. Laktationswoche bis zur 6. Woche der folgenden Laktation durchgeführt. Für 4 Tiere je Behandlung entsprach die P?Versorgung entweder den Empfehlungen des NRC (1981) oder sie war auf die Höhe des angenommenen Nettobedarfes reduziert, während die Ca?Versorgung entweder normgerecht oder in doppelter Höhe erfolgte. Futterverzehr, Verlauf der Lebendmasse, Milchleistung, Fortpflanzung und Verdaulichkeit der organischen Substanz wurden nicht signifikant durch den Gehalt von P oder Ca im Futter beeinflußt. Die Reduktion der P?Versorgung bewirkte im Blutplasma eine Senkung des Phosphat?Gehaltes und einen Anstieg des Calcium?Gehaltes, während die Verdoppelung der Ca?Versorgung ohne Wirkung auf beide Werte blieb. P? und Ca?Bilanzen wurden in 7 Phasen des Versuches bestimmt. Zu Beginn verloren die Ziegen bei reduzierter P?Versorgung täglich 0,2?0,6 g P, später unterschied sich die P?Bilanz dieser Tiere nicht von derjenigen der angemessen mit P versorgten Tiere. Die Höhe der Ca?Aufnahme hatte keinen Effekt auf die P?Bilanz. Die Ca?Bilanz wurde weder durch die Ca?Aufnahme noch durch die P?Aufnahme signifikant beeinflußt.},
	number = {1‐5},
	urldate = {2018-03-26},
	journal = {Journal of Animal Physiology and Animal Nutrition},
	author = {{Deitert C.} and {Pfeffer E.}},
	year = {2009},
	pages = {12--21}
}

@article{tait_variability_1996,
	series = {N. {American} {Nutrition} {Conferences}},
	title = {Variability in individual animal's intake of minerals offered free-choice to grazing ruminants},
	volume = {62},
	issn = {0377-8401},
	url = {http://www.sciencedirect.com/science/article/pii/S0377840196010073},
	doi = {10.1016/S0377-8401(96)01007-3},
	abstract = {A review of the literature indicates that there is very little published information on the variation in intake of minerals and other supplements offered free-choice to grazing animals. Most of the studies have been conducted with molasses feedblocks and liquid licks and the data indicate large day to day and among animal variation. A computer recording system has been developed and tested which allows for more information to be collected with regard to frequency of consumption of free-choice supplements by grazing animals. Studies have been conducted with molasses blocks and loose mineral supplements provided to grazing cattle. Results indicate that the supplements were consumed by almost all animals on a regular basis. However, there was considerable variation among animals in the quantity of supplement consumed. For the molasses blocks daily intake averaged 1.2 ± 0.6 kg per head within a range of 0.72–1.65 kg. For the mineral supplement the average daily consumption was 135 ± 55 g per head within a range of 50–300 g. The supplement feeders were visited every hour of the day but approximately 40\% of the visits occurred in the late evening. Information on the behavioural pattern of consumption may enable strategies to be developed which could reduce the variation in consumption.},
	number = {1},
	urldate = {2018-03-23},
	journal = {Animal Feed Science and Technology},
	author = {Tait, R. M. and Fisher, L. J.},
	year = {1996},
	keywords = {Minerals, Feedblocks, Grazing animals, Supplements},
	pages = {69--76}
}

