	year = {2011},
	keywords = {milk fever, metabolism, Osteocalcin, lactation, cattle, growth, Hypocalcaemia, Bone metabolic marker, bone turnover, Cow, dry   period, gla protein, phosphatase-activity, pregnancy},
	pages = {196--198},
