	file = {Odongo et al. - 2007 - Long-term effects of feeding diets without mineral.pdf:C\:\\Users\\p.gaignon\\Zotero\\storage\\K9PTMTAZ\\Odongo et al. - 2007 - Long-term effects of feeding diets without mineral.pdf:application/pdf}
