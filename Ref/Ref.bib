@article{goff_monitoring_2008,
	series = {Special {Issue}: {Production} {Diseases} of the {Transition} {Cow}},
	title = {The monitoring, prevention, and treatment of milk fever and subclinical hypocalcemia in dairy cows},
	volume = {176},
	issn = {1090-0233},
	url = {http://www.sciencedirect.com/science/article/pii/S1090023307004248},
	doi = {10.1016/j.tvjl.2007.12.020},
	abstract = {The periparturient cow undergoes a transition from non-lactating to lactating at calving. The animal is tremendously challenged to maintain calcium homeostasis. Those that fail can develop milk fever, a clinical disorder that is life threatening to the cow and predisposes the animal to a variety of other disorders. Guidelines for monitoring the incidence of hypocalcemia and methods for treating milk fever are reviewed. The physiological factors that cause milk fever and strategies for prevention of milk fever are discussed, focusing on the effects diet cation–anion difference can have on tissue sensitivity to parathyroid hormone. Another major risk factor for milk fever is hypomagnesemia, which is observed when animals are fed inadequate amounts of magnesium, or some factor is present in the diet that prevents adequate absorption of magnesium. Moderate hypomagnesemia impairs the ability of the cow to maintain calcium homeostasis and hypocalcemia occurs.},
	number = {1},
	urldate = {2016-07-04},
	journal = {The Veterinary Journal},
	author = {Goff, Jesse P.},
	year = {2008},
	keywords = {milk fever, hypocalcemia, Anionic salts, DCAD, Hypomagnesemia},
	pages = {50--57}
}

